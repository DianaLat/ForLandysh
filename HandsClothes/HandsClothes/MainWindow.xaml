<Window x:Class="HandsClothes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HandsClothes"
        mc:Ignorable="d"
        Title="Одежда для ручек" Height="600" Width="800" MinHeight="600" MinWidth="800" Background="#B3F4E9">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="11*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Left" Orientation="Horizontal">
            <Image Name="Image" Source="Images\Одежда для ручек.png" Margin="5"/>
            <TextBlock Text="Одежда для ручек" Margin="5" VerticalAlignment="Center" FontSize="25" />
        </StackPanel>
        <Frame Grid.Row="1" Margin="5" Name="Contenter" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
